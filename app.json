{
  "expo": {
    "name": "appDoc-front",
    "slug": "appDoc-front",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/loadingPage.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/loadingPage.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "com.kelly62.appDocfront",
      "infoPlist": {
        "NSMicrophoneUsageDescription": "Access the microphone.",
        "NSCameraUsageDescription": "Access the camera.",
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true,
          "NSExceptionDomains": {
            "beatmate.zapto.org": {
              "NSIncludesSubdomains": true,
              "NSExceptionAllowsInsecureHTTPLoads": true
            }
          }
        }
      },
      "permissions": {
        "camera": true,
        "audioRecording": true,
        "mediaLibrary": true
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/loadingPage.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.kelly62.appDocfront"
    },
    "web": {
      "favicon": "./assets/loadingPage.png"
    },
    "extra": {
      "eas": {
        "projectId": "9e8d182b-e2ec-42e0-b634-9cb44b20047c"
      },
      "expo-cli": {
        "proxy": "https://beatmate.zapto.org"
      },
      "api": {
        "baseUrl": "https://beatmate.zapto.org"
      }
    }
  }
}
